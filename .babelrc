{
    "presets": [
        [
            "env",
            {
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "not ie <= 8"
                    ]
                }
            }
        ],
        "stage-2"
    ],
    "plugins": [
        [
            "transform-runtime"
        ],
        [
            "component",
           
                {
                    "libraryName": "mint-ui",
                    "style": true
                }
            
        ],
        [
            "transform-decorators-legacy"
        ],
        [
            "transform-vue-jsx"
        ]
    ]
}